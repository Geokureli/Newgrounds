<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="Newgrounds.io" package="io.newgrounds.test.Main" version="1.0.0" company="GeoKureli" />
	<app main="io.newgrounds.test.Main" path="Export" file="Newgrounds" />
	<app preloader="io.newgrounds.test.TestPreloader" />
	
	<window width="800" height="800" fps="30" background="#4C4848" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	<haxelib name="swf" unless="simple" />
	<!--<haxelib name="flixel" />-->
	<classpath path="../lib/Source" />
	
	<!-- <haxedef name="simulate_preloader" if="debug"/> -->
	<!-- <haxedef name="ng_lite" /> -->
	
	<section unless="simple">
		<library path="Assets/Test Layout.swf" id="layout" generate="true" preload="true" />
		<assets path="Assets/fonts" rename="fonts" embed="true" />
	</section>
	
</project>
